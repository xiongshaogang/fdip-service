package com.trusdom.fdip.vo;

import java.math.BigDecimal;

public class IncomeVo extends BaseVo {

	//昨日收益
	private BigDecimal ystdayIncome = new BigDecimal(0);
	
	//累计收益
	private BigDecimal income = new BigDecimal(0);
	
	//万份收益
	private BigDecimal millionIncome = new BigDecimal(0);

	public BigDecimal getYstdayIncome() {
		return ystdayIncome;
	}

	public void setYstdayIncome(BigDecimal ystdayIncome) {
		this.ystdayIncome = ystdayIncome;
	}

	public BigDecimal getIncome() {
		return income;
	}

	public void setIncome(BigDecimal income) {
		this.income = income;
	}

	public BigDecimal getMillionIncome() {
		return millionIncome;
	}

	public void setMillionIncome(BigDecimal millionIncome) {
		this.millionIncome = millionIncome;
	}
	
	
}
